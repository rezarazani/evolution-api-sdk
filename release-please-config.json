{
    "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
    "bootstrap-sha": "",
    "include-v-in-tag": true,
    "plugins": [
        {
            "type": "node-workspace"
        }
    ],
    "packages": {
        ".": {
            "release-type": "node",
            "extra-files": [
                {
                    "type": "json",
                    "path": "package.json",
                    "jsonpath": "$.version"
                }
            ]
        }
    }
}
